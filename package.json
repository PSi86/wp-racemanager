{
    "name": "wp-racemanager",
    "version": "1.0.1",
    "scripts": {
      "build": "wp-scripts build --source-path=blocks-src --output-path=blocks && npm run copy-json",
      "start": "wp-scripts start --source-path=blocks-src --output-path=blocks",
      "copy-json": "copyfiles -u 1 \"blocks-src/**/block.json\" blocks/"
    },
    "devDependencies": {
      "@wordpress/block-editor": "^14.17.0",
      "@wordpress/blocks": "^14.11.0",
      "@wordpress/components": "^29.8.0",
      "@wordpress/core-data": "^7.22.0",
      "@wordpress/data": "^10.22.0",
      "@wordpress/element": "^6.22.0",
      "@wordpress/i18n": "^5.22.0",
      "@wordpress/scripts": "^30.15.0",
      "copyfiles": "^2.4.1"
    }
  }
  